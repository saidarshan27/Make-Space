{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console":"warn",
        "no-unused-vars":"warn",
        "func-names":"off",
        "arrow-body-style":"off",
        "no-restricted-syntax":"off"
    }
}
